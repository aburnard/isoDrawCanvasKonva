set image only when it is loaded\n      this.brick = brick;\n    };\n\n    let sharedImageObj = new window.Image();\n    sharedImageObj.onload = () => {\n      // set image only when it is loaded\n      this.sharedImageObj = sharedImageObj;\n    };\n    sharedImageObj.src = this.sharedImage;\n  }\n};\n</script>\n\n<style>\nbody {\n  margin: 0;\n  padding: 0;\n}\n</style>"],"sourceRoot":"src/components"}]}